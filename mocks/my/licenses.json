{
  "expression": "/graphql",
  "body": "(.*)query getMy(.*)",
  "method": "POST",
  "name": "my",
  "responses": {
    "noValid": {
      "status": 200,
      "delay": 200,
      "file": "mocks/my/data/noValid.json"
    },
    "oneValid": {
      "status": 200,
      "delay": 200,
      "file": "mocks/my/data/oneValid.json"
    },
    "twoValid": {
      "status": 200,
      "delay": 200,
      "file": "mocks/my/data/twoValid.json"
    },
    "validInFuture": {
      "default": true,
      "status": 200,
      "delay": 200,
      "file": "mocks/my/data/validInFuture.json"
    },
    "500": {
      "status": 500,
      "delay": 200
    }
  }
}
